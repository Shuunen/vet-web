    "preview": "vite preview",
    "sb": "storybook dev -p 6006",
    "test": "vitest --coverage --run && echo test success",
    "test:bench": "vitest bench",
    "test:sb": "test-storybook --coverage",
    "test:update": "vitest --run --update && echo test update success",
