    "management"
  ],
  "license": "MIT",
  "name": "vet-web",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@swc/core",
      "@tailwindcss/oxide",
      "esbuild",
      "msw",
      "oxc-resolver"
    ]
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/vet-web.git"
  },
  "scripts": {
    "build": "tsc -b && vite build && echo build success",
    "build:mark": "unique-mark dist/index.{html} && echo build:mark success",
    "build:sb": "storybook build && echo build:sb success",
    "check": "pnpm i && echo install success && pnpm check:tsc && pnpm check:biome && pnpm check:oxlint && pnpm check:knip && pnpm build && pnpm run test && echo check success",
    "check:biome": "biome check --error-on-warnings --write --unsafe src && echo check:biome success",
    "check:knip": "knip && echo check:knip success",
    "check:oxlint": "oxlint src --deny-warnings --fix --fix-suggestions --fix-dangerously && echo check:oxlint success",
    "check:repo": "repo-check && echo check:repo success",
    "check:tsc": "tsc --noEmit --project tsconfig.app.json --incremental && echo check:tsc success",
    "dev": "vite",
    "postversion": "git push && git push --tags",
    "preversion": "pnpm check",
    "preview": "vite preview",
    "sb": "storybook dev -p 6006",
    "shad:update": "for file in src/components/ui/*.tsx; do pnpm dlx shadcn@latest add -y -o $(basename \"$file\" .tsx); done",
    "test": "vitest --coverage --run && echo test success",
    "test:bench": "vitest bench",
    "test:sb": "test-storybook --coverage",
    "test:update": "vitest --run --update && echo test update success",
    "test:watch": "vitest --coverage"
  },
  "type": "module",
  "version": "0.0.0"
}